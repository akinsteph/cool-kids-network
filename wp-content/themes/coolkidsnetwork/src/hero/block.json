{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "cool-kids-network/hero",
	"title": "Hero",
	"category": "cool-kids-network",
	"icon": "superhero",
	"description": "A hero section for the Cool Kids Network theme.",
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string",
			"default": "Welcome to Cool Kids Network"
		},
		"description": {
			"type": "string",
			"default": "Make your own cartoon character and play fun games with lots of other kids!"
		},
		"content": {
			"type": "string",
			"default": ""
		},
		"alignment": {
			"type": "string",
			"default": "center"
		},
		"backgroundColor": {
			"type": "string",
			"default": "#000000"
		},
		"backgroundImage": {
			"type": "string",
			"default": ""
		},
		"loggedOutButtons": {
			"type": "array",
			"default": [
				{
					"text": "Sign Up",
					"link": "",
					"linkType": "page"
				}
			]
		},
		"loggedInButtons": {
			"type": "array",
			"default": [
				{
					"text": "My character",
					"link": "",
					"linkType": "page"
				}
			]
		}
	},
	"textdomain": "coolkidsnetwork",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"viewStyle": "file:./style-index.css"
}